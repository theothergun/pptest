        ],
        "language_manager": [
          "user",
          "admin"
        ],
        "packaging": [
          "user"
        ],
        "reports": [
          "user",
          "admin"
        ],
        "route_settings": [
          "user",
          "admin"
        ],
        "settings": [
          "user"
        ],
        "summary": [
          "user",
          "admin"
        ],
        "tcp_settings": [
          "admin"
        ],
        "vi_home": [
          "user",
          "admin"
        ]
      },
      "visible_routes": [
        "summary",
        "packaging",
        "settings",
        "errors",
        "vi_home",
        "test5",
        "app_state",
        "language_manager"
      ]
    }
  },
  "workers": {
    "configs": {
      "com_device": {
        "devices": [
          {
            "baudrate": 115200,
            "bytesize": 8,
            "delimiter": "\r\n",
            "device_id": "scanner_1",
            "encoding": "utf-8",
            "max_line_len": 4096,
            "mode": "line",
            "parity": "N",
            "port": "COM5",
            "read_chunk_size": 256,
            "reconnect_max_s": 5.0,
            "reconnect_min_s": 0.5,
            "stopbits": 1.0,
            "timeout_s": 0.2,
            "write_timeout_s": 0.5
          }
        ]
      },
      "itac": {
        "endpoints": [
          {
            "auto_login": true,
            "base_url": "http://mk01smesapp02:8080/mes/imsapi/rest/actions",
            "client": "01",
            "name": "itac_main",
            "registration_type": "S",
            "station_number": "6000000001",
            "system_identifier": "nicegui",
            "timeout_s": 10.0,
            "verify_ssl": false
          }
        ]
      },
      "rest_api": {
        "endpoints": [
          {
            "base_url": "https://api.example.com",
            "headers": {
              "Authorization": "Bearer token"
            },
            "name": "example",
            "timeout_s": 10.0,
            "verify_ssl": true
          },
          {
            "base_url": "http://127.0.0.1:8088/system/webdev/Core/odc/BookSingleSerial",
            "headers": {
              "Content-Type": "application/json"
            },
            "name": "book_serial",
            "timeout_s": 10.0,
            "verify_ssl": false
          }
        ]
      },
      "script_worker": {
        "auto_start_chains": [
          {
            "instance_id": "default",
            "script_name": "examples/popups"
          }
        ]
      },
      "tcp_client": {
        "clients": [
          {
            "auto_reconnect": true,
            "client_id": "scanner_1",
            "connect": true,
            "delimiter": "\\n",
            "encoding": "utf-8",
            "host": "127.0.0.1",
            "keepalive": true,
            "mode": "line",
            "port": 4001,
            "reconnect_max_s": 10.0,
            "reconnect_min_s": 1.0,
            "tcp_nodelay": true
          }
        ]
      },
      "twincat": {
        "plc_endpoints": [
          {
            "ads_port": 851,
            "client_id": "twincat",
            "plc_ams_net_id": "5.140.130.124.1.1",
            "plc_ip": "192.168.0.50",
            "subscriptions": [
              {
                "name": "MAIN.currentIPAddress",
                "plc_type": "STRING",
                "string_len": 64
              },
              {
                "name": "MAIN.watchdog",
                "plc_type": "UINT"
              },
              {
                "name": "MAIN.test",
                "plc_type": "STRING"
              },
              {
                "name": "MAIN.idx",
                "plc_type": "INT"
              },
              {
                "name": "MAIN.isMQTTSetup",
                "plc_type": "BOOL"
              }
            ]
          }
        ]
      }
    },
    "enabled_workers": [
      "script_worker",
      "twincat",
      "tcp_client",
      "itac",
      "rest_api",
      "com_device"
    ]
  }
}