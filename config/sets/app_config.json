        "clients": []
      },
      "twincat": {
        "plc_endpoints": [
          {
            "ads_port": 851,
            "client_id": "packaging_station",
            "default_cycle_ms": 200,
            "default_string_len": 80,
            "default_trans_mode": "server_cycle",
            "plc_ams_net_id": "5.140.130.124.1.1",
            "plc_ip": "192.168.1.50",
            "subscriptions": [
              {
                "alias": "step",
                "name": "MAIN.module.zenonVisu.StepText",
