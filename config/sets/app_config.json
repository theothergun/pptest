        "settings": [
          "user"
        ],
        "tcp_settings": [
          "admin"
        ],
        "vi_home": [
          "user",
          "admin"
        ]
      },
      "visible_routes": [
        "summary",
        "packaging",
        "settings",
        "errors",
        "vi_home",
        "test5",
        "app_state",
        "language_manager"
      ]
    }
  },
  "workers": {
    "configs": {
      "com_device": {
        "devices": []
      },
      "itac": {
        "endpoints": []
      },
      "opcua": {
        "endpoints": [
          {
            "auto_connect": true,
            "name": "local",
            "nodes": [
              {
                "alias": "time",
                "node_id": "ns=2;s=[System]/Gateway/CurrentDateTime",
                "poll_ms": 500
              },
              {
                "alias": "Watchdog",
                "node_id": "ns=2;s=[core_rscheuer]/KE-Elektronik/MK01/DataWiring/TransferLine/Fakra-KE-27/MES/SetupCheck/result_text",
                "poll_ms": 250
              }
            ],
            "password": "",
            "security_mode": "None",
            "security_policy": "None",
            "server_url": "opc.tcp://192.168.1.26:62541",
            "timeout_s": 5.0,
            "username": ""
          }
        ]
      },
      "rest_api": {
        "endpoints": [
          {
            "base_url": "https://api.example.com",
            "headers": {
              "Authorization": "Bearer token"
            },
            "name": "example",
            "timeout_s": 10.0,
            "verify_ssl": true
          },
          {
            "base_url": "http://127.0.0.1:8088/system/webdev/Core/odc/BookSingleSerial",
            "headers": {
              "Content-Type": "application/json"
            },
            "name": "book_serial",
            "timeout_s": 10.0,
            "verify_ssl": false
          }
        ]
      },
      "script_worker": {
        "auto_start_chains": [
          {
            "instance_id": "default",
            "script_name": "Packaging/pack"
          }
        ]
      },
      "tcp_client": {
        "clients": []
      },
      "twincat": {
        "plc_endpoints": []
      }
    },
    "enabled_workers": [
      "script_worker",
      "opcua"
    ]
  }
}