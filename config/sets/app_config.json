  "persistence": {
    "backend": "json",
    "external_db": {
      "dsn": "",
      "enabled": false
    },
    "json_path": "config/app_config.json"
  },
  "proxy": {
    "enabled": false,
    "http": "http://192.168.1.26:3128",
    "https": "http://192.168.1.26:3128",
    "no_proxy": "localhost,127.0.0.1,10.1.2.3"
  },
  "ui": {
    "navigation": {
      "custom_routes": [
        {
          "icon": "home",
          "key": "home",
          "label": "Home",
          "path": "home.py"
        },
        {
          "icon": "warning",
          "key": "errors",
          "label": "Errors",
          "path": "errors.py"
        },
        {
          "icon": "settings",
          "key": "settings",
