        },
        {
          "icon": "dashboard",
          "key": "summary",
          "label": "Summary",
          "path": "summary.py"
        },
        {
          "icon": "home",
          "key": "app_state",
          "label": "app_state",
          "path": "settings/app_state_view.py"
        },
        {
          "icon": "insert_drive_file",
          "key": "packaging",
          "label": "Packaging",
          "path": "operator/packaging.py"
        },
        {
          "icon": "home",
          "key": "vi_home",
          "label": "Visual Inspection",
          "path": "visual_inspection/home.py"
        },
        {
          "icon": "lang",
          "key": "language_manager",
