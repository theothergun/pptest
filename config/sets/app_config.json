        },
        {
          "icon": "warning",
          "key": "errors",
          "label": "Errors",
          "path": "errors.py"
        },
        {
          "icon": "settings",
          "key": "settings",
          "label": "Settings",
          "path": "settings/settings_page.py"
        },
        {
          "icon": "insert_drive_file",
          "key": "packaging",
          "label": "Packaging",
          "path": "operator/packaging.py"
        },
        {
          "icon": "home",
          "key": "vi_home",
          "label": "Visual Inspection",
          "path": "visual_inspection/home.py"
        },
        {
          "icon": "lang",
          "key": "language_manager",
          "label": "Langauge",
          "path": "settings/language_manager.py"
        }
      ],
      "dark_mode": true,
      "hide_nav_on_startup": false,
      "main_route": "packaging",
      "route_roles": {
        "errors": [
          "user",
          "admin"
        ],
        "home": [
          "user",
          "admin"
        ],
        "language_manager": [
          "user",
