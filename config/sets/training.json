{
  "auth": {
    "login_required": false
  },
  "persistence": {
    "backend": "json",
    "external_db": {
      "dsn": "",
      "enabled": false
    },
    "json_path": "config/sets/training.json"
  },
  "ui": {
    "navigation": {
      "custom_routes": [
        {
          "icon": "school",
          "key": "training_example",
          "label": "Training Example",
          "path": "training/training_example_view.py"
        },
        {
          "icon": "settings",
          "key": "settings",
          "label": "Settings",
          "path": "settings/settings_page.py"
        }
      ],
      "main_route": "training_example",
      "route_roles": {},
      "visible_routes": [
        "training_example",
        "settings"
      ]
    }
  },
  "workers": {
    "configs": {
      "script_worker": {
        "auto_start_chains": []
      },
      "tcp_client": {
        "clients": [
          {
            "auto_reconnect": false,
            "client_id": "training_scanner",
            "connect": false,
            "delimiter": "\\n",
            "encoding": "utf-8",
            "host": "127.0.0.1",
            "keepalive": false,
            "mode": "line",
            "port": 4001,
            "reconnect_max_s": 1.0,
            "reconnect_min_s": 1.0,
            "tcp_nodelay": true
          }
        ]
      }
    },
    "enabled_workers": [
      "script_worker",
      "tcp_client"
    ]
  }
}
